---
import { Separator } from "@/components/Separator/Separator";
import { fetchGlobal } from "@/lib/getGlobal";
import type { Footer as FooterType } from "@/lib/payloadTypes";
import { cn } from "@/utils/ui";

import type { PayloadResponse, Post } from "@/lib/payloadTypes";
import { payloadFetch } from "@/utils/payloadFetch";
import PostCard from "@/components/PostCard/PostCard.astro";

const response = await payloadFetch<PayloadResponse<Post>>(
    `/posts?limit=1&sort=-publishedAt&depth=2`,
);
const { docs } = response;
const post = docs[0] as Post;
const hasCategories = post.categories && post.categories.length > 0;
const hasTags = post.tags && post.tags.length > 0;

const footerData = await fetchGlobal<FooterType>("footer");
const { navItems, separatorType, isReverse, bottom, enableBackgroundImage } =
    footerData as FooterType;
---

<footer class={cn("elative pt-40", enableBackgroundImage ? "bg-section" : "")}>
    <div class="wrapper m-auto grid grid-cols-3 gap-4">
        <div class="grid grid-rows-[3rem_1fr]">
            <h3 class="justify-self-start">Educar Sano</h3>
            <nav>
                <ul class="flex flex-col gap-2">
                    {
                        navItems &&
                            navItems.map((item) => {
                                const { link, id } = item;
                                return <li>{link.label}</li>;
                            })
                    }
                </ul>
            </nav>
        </div>
        <div class="grid grid-rows-[3rem_1fr]">
            <h3 class="justify-self-start">último Post</h3>
            <div>
                <h4>Último Post</h4>
                <PostCard
                    post={post}
                    hasCategories={hasCategories!}
                    hasTags={hasTags!}
                />
            </div>
        </div>
        <div class="grid grid-rows-[3rem_1fr]">
            <h3 class="justify-self-start">Contacto</h3>
            <div>
                <span>contacto@educarsano.com</span>
            </div>
        </div>
    </div>
    <div class="bg-primary py-4 grid grid-rows-2 place-items-center">
        <div class="bg-white px-2">
            <svg
                width="60"
                height="60"
                viewBox="0 0 384 222"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
